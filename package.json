{
	"name": "sketch-nudge-font-size",
	"description": "Quickly increase or decrease the selected text layer(s) font size based on available shared text styles from a library",
	"version": "0.2.0",
	"engines": {
		"sketch": ">=49.0"
	},
	"skpm": {
		"name": "sketch-nudge-font-size",
		"manifest": "src/manifest.json",
		"main": "sketch-nudge-font-size.sketchplugin",
		"assets": [
			"assets/**/*"
		],
		"sketch-assets-file": "sketch-assets/icons.sketch"
	},
	"scripts": {
		"build": "skpm-build",
		"watch": "skpm-build --watch",
		"start": "skpm-build --watch --run",
		"postinstall": "npm run build && skpm-link"
	},
	"devDependencies": {
		"@skpm/builder": "^0.9.5"
	}
}
